<% layout('layouts/boilerplate')%>
    <H1>View All Ngos in Data base</H1>
    <div>

             <%for (let n of ngos){%>
                
                <div class="card m-auto my-3" style="width: 70%;">
                     <img src="..." class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title"><%= n.ngoName%></h5>
                            <p class="card-text"><%=n._id%></p>
                            <a href="/show/<%=n._id%>" class="btn btn-primary">Edit <%= n.ngoName%></a>
                        </div>
                </div>
            <%}%>
            
 
    </div>

        <div>
            <a href="/show/new">Register</a>
        </div>
